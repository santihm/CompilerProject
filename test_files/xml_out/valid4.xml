                        </UMinusOp>
                    </TermOp>
                </AssignOp>
                <AssignOp>
                    identifier: y
                    <TermOp>
                        <UMinusOp>
                            int_const: 1
                        </UMinusOp>
                    </TermOp>
                </AssignOp>
                <WhileOp>
                    <TermOp>
                        <NotOp>
                            <GTOp>
                                <TermOp>
                                    identifier: x
                                </TermOp>
                                <TermOp>
                                    int_const: 0
                                </TermOp>
                            </GTOp>
