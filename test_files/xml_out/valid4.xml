        </VarDeclOp>
        <VarDeclOp>
            <VarOptInitOp>
                identifier: sommagrande
            </VarOptInitOp>
            <VarOptInitOp>
                identifier: sommapiccola
            </VarOptInitOp>
            double
        </VarDeclOp>
