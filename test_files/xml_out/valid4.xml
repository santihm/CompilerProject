                            </TermOp>
                        </WriteOp>
                        <AssignOp>
                            identifier: sommapiccola
                            <AddOp>
                                <TermOp>
                                    identifier: sommapiccola
                                </TermOp>
                                <TermOp>
                                    identifier: risultato
                                </TermOp>
                            </AddOp>
                        </AssignOp>
                    </BodyOp>
                </IfThenElseOp>
                <AssignOp>
                    identifier: i
                    <DiffOp>
                        <TermOp>
                            identifier: i
                        </TermOp>
                        <TermOp>
                            int_const: 1
                        </TermOp>
                    </DiffOp>
                </AssignOp>
