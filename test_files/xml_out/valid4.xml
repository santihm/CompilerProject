            </TermOp>
        </AssignOp>
        <WhileOp>
            <LEOp>
                <TermOp>
                    identifier: i
                </TermOp>
                <TermOp>
                    int_const: 0
                </TermOp>
            </LEOp>
            <BodyOp>
                <VarDeclOp>
                    <VarOptInitOp>
                        identifier: saluto
                    </VarOptInitOp>
