                        <WriteOp>
                            <TermOp>
                                identifier: saluto
                            </TermOp>
                        </WriteOp>
                    </BodyOp>
                </WhileOp>
                <CallOp>
                    identifier: moltiplicazione
                    <TermOp>
                        identifier: x
                    </TermOp>
                    <TermOp>
                        identifier: y
                    </TermOp>
                    <TermOp>
                        identifier: risultato
