                        <TermOp>
                            identifier: stringa
                        </TermOp>
                    </AddOp>
                </ReturnOp>
            </BodyOp>
        </DefDeclOp>
        <DefDeclOp>
            identifier: somma
            void
            <ParDeclOp>
                <PVarOp>
                    identifier: input1
                </PVarOp>
                double
            </ParDeclOp>
            <ParDeclOp>
                <PVarOp>
                    identifier: input2
                </PVarOp>
                double
            </ParDeclOp>
            <ParDeclOp>
                <PVarOp>
                    identifier: result
