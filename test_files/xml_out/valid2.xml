                            </TermOp>
                            <TermOp>
                                identifier: input2
                            </TermOp>
                            <TermOp>
                                identifier: result
                            </TermOp>
                        </CallOp>
                    </BodyOp>
                    <BodyOp>
                        <IfThenElseOp>
                            <EQOp>
                                <TermOp>
                                    identifier: operazione
                                </TermOp>
                                <TermOp>
                                    string_const: "sottrazione"
                                </TermOp>
                            </EQOp>
                            <BodyOp>
                                <CallOp>
                                    identifier: sottrazione
                                    <TermOp>
                                        identifier: input1
                                    </TermOp>
                                    <TermOp>
                                        identifier: input2
                                    </TermOp>
                                    <TermOp>
                                        identifier: result
                                    </TermOp>
                                </CallOp>
                            </BodyOp>
                            <BodyOp>
                                <IfThenElseOp>
                                    <EQOp>
                                        <TermOp>
                                            identifier: operazione
                                        </TermOp>
                                        <TermOp>
                                            string_const: "divisione"
                                        </TermOp>
                                    </EQOp>
                                    <BodyOp>
                                        <AssignOp>
                                            identifier: result
