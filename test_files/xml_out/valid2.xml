                        </WriteOp>
                        <ReturnOp>
                            <TermOp>
                                double_const: 0.0
                            </TermOp>
                        </ReturnOp>
                    </BodyOp>
                </IfThenOp>
                <ReturnOp>
                    <TermOp>
                        identifier: input1
                        <DivOp>
                            identifier: input2
                        </DivOp>
                    </TermOp>
                </ReturnOp>
            </BodyOp>
        </DefDeclOp>
        <DefDeclOp>
            identifier: moltiplicazione
            double
            <ParDeclOp>
                <PVarOp>
                    identifier: input1
                </PVarOp>
                double
            </ParDeclOp>
            <ParDeclOp>
                <PVarOp>
                    identifier: input2
                </PVarOp>
                double
            </ParDeclOp>
            <BodyOp>
                <ReturnOp>
                    <TermOp>
                        identifier: input1
                        <MulOp>
                            identifier: input2
                            Next TermTailOp null
                        </MulOp>
                    </TermOp>
                </ReturnOp>
            </BodyOp>
        </DefDeclOp>
        <DefDeclOp>
            identifier: sottrazione
            void
