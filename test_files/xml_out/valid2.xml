        <DefDeclOp>
            identifier: divisione
            double
            <ParDeclOp>
                <PVarOp>
                    identifier: input1
                </PVarOp>
                double
            </ParDeclOp>
            <ParDeclOp>
                <PVarOp>
                    identifier: input2
                </PVarOp>
                double
            </ParDeclOp>
            <BodyOp>
                <IfThenOp>
                    <EQOp>
                        <TermOp>
                            identifier: input2
